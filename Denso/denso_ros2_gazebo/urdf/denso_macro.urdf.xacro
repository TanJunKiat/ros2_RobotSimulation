    <plugin name="gazebo_ros_ft_sensor" filename="libgazebo_ros_ft_sensor.so">
      <ros>
        <namespace>/loadcell</namespace>
        <remapping>wrench:=gamma</remapping>
      </ros>
      <body_name>denso_link6</body_name>
      <frame_name>gamma_frame</frame_name>
      <update_rate>10</update_rate>
      <gaussian_noise>0.0000001</gaussian_noise>
    </plugin>
    </gazebo>
    -->
    
  </xacro:macro>
</robot>
