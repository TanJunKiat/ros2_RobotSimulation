<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <!-- XACRO-MACRO of DENSO ROBOT: -->
  <xacro:macro name="denso" params="prefix EE_no">
  
    <!-- Load DENSO ROBOT TRANSMISSION file: -->
    <xacro:include filename="$(find denso_ros2_gazebo)/urdf/denso_transmission.xacro" />
    <xacro:denso_transmission 
      prefix=""
    />
    
    <!-- Load DENSO ROBOT ROS2_CONTROL file: -->
    <xacro:include filename="$(find denso_ros2_gazebo)/urdf/denso_ros2control.xacro" />
    <xacro:denso_ros2control 
      prefix=""
      EE_no="${EE_no}"
    />

    <!-- ***** DENSO ROBOT .urdf SERIAL CHAIN ***** -->
    
    <!-- denso_link0 -->
    
    <link name="denso_link0">
      <inertial>
        <mass value="2.92"/>
        <origin rpy="0 0 0" xyz="0 0 0.1545"/>
        <inertia ixx="0.00782229414331" ixy="-1.56191622996e-05" ixz="-0.00126005738123" iyy="0.0109027971813" iyz="1.08233858202e-05" izz="0.0102355503949"/>
      </inertial>
      <visual>
        <origin rpy="0 0 0" xyz="0 0 0.1545"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/visual/link0.dae"/>
        </geometry>
        <material name="">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0.1545"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/collision/link0.dae"/>
        </geometry>
      </collision>
    </link>

    <!-- denso_link1 -->
    <link name="denso_link1">
      <inertial>
        <mass value="2.74"/>
        <origin rpy=" 1.57075 0 0" xyz="0.0 0 0.704"/>
        <inertia ixx="0.0180416958283" ixy="0.0" ixz="0.0" iyy="0.0159136071891" iyz="0.0046758424612" izz="0.00620690827127"/>
      </inertial>
      <visual>
        <origin rpy=" 1.57075 0 0" xyz="0.0 0 0.704"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/visual/link1.STL"/>
        </geometry>
        <material name="">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy=" 1.57075 0 0" xyz="0.0 0 0.704"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/collision/link1.STL"/>
        </geometry>
      </collision>
    </link>
    <!-- denso_joint1 -->
    <joint name="denso_joint1" type="revolute">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="denso_link0"/>
      <child link="denso_link1"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-7.0" upper="7.0" velocity="4.36332"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>
    
    <!-- denso_link2 -->
    <link name="denso_link2">
      <inertial>
        <mass value="2.74"/>
        <origin rpy="3.141592 0 0" xyz="0 0 0"/>
        <inertia ixx="0.0182856182281" ixy="0.0" ixz="0.0" iyy="0.00621358421175" iyz="-0.00472844221905" izz="0.0161514346309"/>
      </inertial>
      <visual>
        <origin rpy="3.141592 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/visual/link2.STL"/>
        </geometry>
        <material name="">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="3.141592 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/collision/link2.STL"/>
        </geometry>
      </collision>
    </link>
    <!-- denso_joint2 -->
    <joint name="denso_joint2" type="revolute">
      <origin rpy="0 0 0" xyz="0 -0.035 0.704"/>
      <parent link="denso_link1"/>
      <child link="denso_link2"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-2.87979" upper="2.87979" velocity="4.36332"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <!-- denso_link3 -->
    <link name="denso_link3">
      <inertial>
        <mass value="2.38"/>
        <origin rpy="0 0 -1.57075" xyz="0 0.021 0"/>
        <inertia ixx="0.00771376630908" ixy="-0.00248490625138" ixz="-0.00332147581033" iyy="0.00989108008727" iyz="-0.00217796151484" izz="0.00811723558464"/>
      </inertial>
      <visual>
        <origin rpy="0 0 -1.57075" xyz="0 0.021 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/visual/link3.STL"/>
        </geometry>
        <material name="">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="0 0 -1.57075" xyz="0 0.021 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/collision/link3.STL"/>
        </geometry>
      </collision>
    </link>
    <!-- denso_joint3 -->
    <joint name="denso_joint3" type="revolute">
      <origin rpy="0 0 0" xyz="0 -0.34 0"/>
      <parent link="denso_link2"/>
      <child link="denso_link3"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-2.87979" upper="2.87979" velocity="4.36332"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <!-- denso_link4-->
    <link name="denso_link4">
      <inertial>
        <mass value="2.38"/>
        <origin rpy="0 0 0" xyz="0 -0.021 0"/>
        <inertia ixx="0.00799663881132" ixy="0.00347095570217" ixz="-0.00241222942995" iyy="0.00825390705278" iyz="0.00235774044121" izz="0.0102515004345"/>
      </inertial>
      <visual>
        <origin rpy="0 0 0" xyz="0 -0.021  0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/visual/link4.STL"/>
        </geometry>
        <material name="">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="0 0 0" xyz="0 -0.021 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/collision/link4.STL"/>
        </geometry>
      </collision>
    </link>
    <!-- denso_joint4 -->
    <joint name="denso_joint4" type="revolute">
      <origin rpy="0 0 0" xyz="0 0.021 0"/>
      <parent link="denso_link3"/>
      <child link="denso_link4"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-2.87979" upper="2.87979" velocity="4.36332"/>
      <dynamics damping="0.0" friction="0.0"/>
      <!-- upper should be -0.0698, but that brakes simulation when initial position for this joint is 0.0 -->
    </joint>

    <!-- denso_link5 -->
    <link name="denso_link5">
      <inertial>
        <mass value="2.74"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <inertia ixx="0.030371374513" ixy="6.50283587108e-07" ixz="-1.05129179916e-05" iyy="0.0288752887402" iyz="-0.00775653445787" izz="0.00444134056164"/>
      </inertial>
      <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/visual/link5.STL"/>
        </geometry>
        <material name="">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/collision/link5.STL"/>
        </geometry>
      </collision>
    </link>
    <!-- denso_joint5 -->
    <joint name="denso_joint5" type="revolute">
      <origin rpy="0 0 0" xyz="0 0.00 0.34"/>
      <parent link="denso_link4"/>
      <child link="denso_link5"/>
      <axis xyz="1 0 0"/>
      <limit effort="0" lower="-2.87979" upper="2.87979" velocity="4.36332"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <!-- denso_link6 -->
    <link name="denso_link6">
      <inertial>
        <mass value="1.0"/>
        <origin rpy="1.57075 0 0" xyz="0 0 0"/>
        <inertia ixx="0.030371374513" ixy="6.50283587108e-07" ixz="-1.05129179916e-05" iyy="0.0288752887402" iyz="-0.00775653445787" izz="0.00444134056164"/>
      </inertial>
      <visual>
        <origin rpy="1.57075 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/visual/link6.STL"/>
        </geometry>
        <material name="">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
      <collision>
        <origin rpy="1.57075 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find denso_ros2_gazebo)/meshes/collision/link6.STL"/>
        </geometry>
      </collision>
    </link>
    <!-- denso_joint6 -->
    <joint name="denso_joint6" type="revolute">
      <origin rpy="0 0 0" xyz="0 0.00 0.085"/>
      <parent link="denso_link5"/>
      <child link="denso_link6"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="-2.87979" upper="2.87979" velocity="4.36332"/>
      <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <!-- End-Effector frame -->
    <!--<link name="end_effector_frame">
      <inertial>
        <mass value="0.01"/>
        <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
      </inertial>
    </link>
    
    <joint name="end_effector_frame_fixed_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <parent link="denso_link6"/>
      <child link="end_effector_frame"/>
    </joint>
    
    <link name="base" />
    <joint name="denso_link0_base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="denso_link0"/>
      <child link="base"/>
    </joint>-->


    
